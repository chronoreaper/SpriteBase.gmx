        oControler.x=0
        oControler.y=0
        oControler.xx=0
        oControler.yy=0
        oControler.level=1
    }
}
else{
    alpha1-=0.1
    if alpha1&lt;=0{
        oControler.menu=0
        global.cur_score=0
        audio_stop_all()
        instance_destroy()
    }
}
